    xsi:schemaLocation="http://www.xmos.com http://www.xmos.com" >
  <Type>Board</Type>
  <Name>sliceKIT Core Board (L2)</Name>
  <BoardId>0008011X</BoardId>
        
    <Declarations>
        <Declaration>tileref tile[2]</Declaration>
    </Declarations>
    
    
    <Packages>
